// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// NextAuth.js Models
model Account {
  id                String  @id @default(cuid())
  userId            String
  type              String
  provider          String
  providerAccountId String
  refresh_token     String? @db.Text
  access_token      String? @db.Text
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String? @db.Text
  session_state     String?

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
}

model Session {
  id           String   @id @default(cuid())
  sessionToken String   @unique
  userId       String
  expires      DateTime
  user         User     @relation(fields: [userId], references: [id], onDelete: Cascade)
}

model User {
  id              String    @id @default(cuid())
  name            String?
  email           String    @unique
  emailVerified   DateTime?
  image           String?
  password        String?
  phone           String?
  dateOfBirth     DateTime?
  gender          String?
  address         String?
  city            String?
  country         String?
  postalCode      String?
  preferredLanguage String @default("tr")
  preferredCurrency String @default("TRY")
  isEmailVerified Boolean @default(false)
  isPhoneVerified Boolean @default(false)
  membershipType  MembershipType @default(BASIC)
  loyaltyPoints   Int       @default(0)
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  lastLoginAt     DateTime?
  isActive        Boolean   @default(true)
  twoFactorEnabled Boolean  @default(false)
  
  // Relations
  accounts           Account[]
  sessions           Session[]
  bookings           Booking[]
  reviews            Review[]
  favorites          Favorite[]
  notifications      Notification[]
  walletTransactions WalletTransaction[]
  aiPreferences      AIPreference?
  transferBookings   TransferBooking[]
  priceAlerts        PriceAlert[]
  trips              Trip[]

  @@map("users")
}

model VerificationToken {
  identifier String
  token      String   @unique
  expires    DateTime

  @@unique([identifier, token])
}

// Travel-specific Models
model Booking {
  id              String      @id @default(cuid())
  userId          String
  bookingType     BookingType
  status          BookingStatus @default(PENDING)
  totalAmount     Decimal
  currency        String      @default("TRY")
  paymentMethod   PaymentMethod
  paymentStatus   PaymentStatus @default(PENDING)
  checkInDate     DateTime?
  checkOutDate    DateTime?
  guestCount      Int?
  specialRequests String?
  bookingReference String     @unique
  metaData        Json?       // Store booking-specific data
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
  
  user            User        @relation(fields: [userId], references: [id])
  reviews         Review[]
  
  @@map("bookings")
}

model Review {
  id          String   @id @default(cuid())
  userId      String
  bookingId   String?
  entityType  String   // hotel, activity, flight, etc.
  entityId    String
  rating      Int      // 1-5
  title       String?
  content     String
  images      String[] @default([])
  isVerified  Boolean  @default(false)
  isBlockchain Boolean @default(false)
  blockchainHash String?
  helpfulVotes Int     @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  
  user        User     @relation(fields: [userId], references: [id])
  booking     Booking? @relation(fields: [bookingId], references: [id])
  
  @@map("reviews")
}

model Favorite {
  id         String   @id @default(cuid())
  userId     String
  entityType String   // hotel, activity, destination, etc.
  entityId   String
  createdAt  DateTime @default(now())
  
  user       User     @relation(fields: [userId], references: [id])
  
  @@unique([userId, entityType, entityId])
  @@map("favorites")
}

model Notification {
  id        String            @id @default(cuid())
  userId    String
  type      NotificationType
  title     String
  message   String
  data      Json?             // Additional notification data
  isRead    Boolean           @default(false)
  createdAt DateTime          @default(now())
  
  user      User              @relation(fields: [userId], references: [id])
  
  @@map("notifications")
}

model WalletTransaction {
  id              String            @id @default(cuid())
  userId          String
  type            TransactionType
  amount          Decimal
  currency        String
  cryptoCurrency  String?           // BTC, ETH, etc.
  cryptoAmount    Decimal?
  status          TransactionStatus @default(PENDING)
  description     String
  reference       String?           @unique
  blockchainTxHash String?
  metaData        Json?
  createdAt       DateTime          @default(now())
  updatedAt       DateTime          @updatedAt
  
  user            User              @relation(fields: [userId], references: [id])
  
  @@map("wallet_transactions")
}

model AIPreference {
  id              String   @id @default(cuid())
  userId          String   @unique
  travelStyle     String[] @default([])
  budgetRange     String?
  preferredDestinations String[] @default([])
  interests       String[] @default([])
  groupSize       String?
  travelFrequency String?
  accommodationType String?
  transportPreference String?
  dietaryRestrictions String[] @default([])
  accessibilityNeeds String[] @default([])
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
  
  user            User     @relation(fields: [userId], references: [id])
  
  @@map("ai_preferences")
}

// Enums
enum MembershipType {
  BASIC
  PREMIUM
  VIP
  ENTERPRISE
}

enum BookingType {
  HOTEL
  FLIGHT
  ACTIVITY
  TOUR
  CAR_RENTAL
  RESTAURANT
  PACKAGE
  AIRPORT_TRANSFER
}

enum BookingStatus {
  PENDING
  CONFIRMED
  CANCELLED
  COMPLETED
  REFUNDED
}

enum PaymentMethod {
  CREDIT_CARD
  DEBIT_CARD
  BANK_TRANSFER
  PAYPAL
  BITCOIN
  ETHEREUM
  USDT
  LOYALTY_POINTS
}

enum PaymentStatus {
  PENDING
  PROCESSING
  COMPLETED
  FAILED
  REFUNDED
  PARTIALLY_REFUNDED
}

enum NotificationType {
  BOOKING_CONFIRMATION
  BOOKING_REMINDER
  PAYMENT_SUCCESS
  PAYMENT_FAILED
  REVIEW_REQUEST
  PROMOTION
  AI_RECOMMENDATION
  SYSTEM_UPDATE
  PRICE_DROP
  PRICE_ALERT
}

enum TransactionType {
  DEPOSIT
  WITHDRAWAL
  PAYMENT
  REFUND
  LOYALTY_EARN
  LOYALTY_REDEEM
}

enum TransactionStatus {
  PENDING
  PROCESSING
  COMPLETED
  FAILED
  CANCELLED
}

// Airport Transfer Models
model AirportTransfer {
  id              String   @id @default(cuid())
  name            String
  description     String   @db.Text
  fromLocation    String   // Airport code or location name
  toLocation      String   // Destination location
  distance        Float    // Distance in KM
  duration        Int      // Duration in minutes
  region          String   // Antalya, Alanya, etc.
  isActive        Boolean  @default(true)
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt

  // Relations
  vehicles        TransferVehicle[]
  bookings        TransferBooking[]

  @@map("airport_transfers")
  @@index([region])
  @@index([fromLocation])
}

model TransferVehicle {
  id              String          @id @default(cuid())
  transferId      String
  vehicleType     VehicleType
  name            String          // e.g., "Mercedes Vito VIP"
  capacity        Int             // Passenger capacity
  luggageCapacity Int             // Luggage capacity
  priceStandard   Decimal         // Standard price
  priceVIP        Decimal         // VIP price
  features        String[]        @default([])
  image           String?
  isAvailable     Boolean         @default(true)
  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  // Relations
  transfer        AirportTransfer @relation(fields: [transferId], references: [id], onDelete: Cascade)
  bookings        TransferBooking[]

  @@map("transfer_vehicles")
  @@index([transferId])
  @@index([vehicleType])
}

model TransferBooking {
  id              String          @id @default(cuid())
  userId          String
  transferId      String
  vehicleId       String
  bookingRef      String          @unique

  // Trip Details
  transferType    TransferType    @default(ONE_WAY)
  pickupLocation  String
  dropoffLocation String
  pickupDate      DateTime
  pickupTime      String
  returnDate      DateTime?
  returnTime      String?

  // Passenger Details
  passengerCount  Int
  luggageCount    Int
  specialRequests String?         @db.Text

  // Flight Details
  flightNumber    String?
  arrivalTime     String?

  // Pricing
  basePrice       Decimal
  totalPrice      Decimal
  currency        String          @default("TRY")
  isVIP           Boolean         @default(false)

  // Status
  status          BookingStatus   @default(PENDING)
  paymentStatus   PaymentStatus   @default(PENDING)
  paymentMethod   PaymentMethod?
  paymentIntentId String?         // Stripe payment intent ID

  // Timestamps
  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  // Relations
  user            User            @relation(fields: [userId], references: [id])
  transfer        AirportTransfer @relation(fields: [transferId], references: [id])
  vehicle         TransferVehicle @relation(fields: [vehicleId], references: [id])

  @@map("transfer_bookings")
  @@index([userId])
  @@index([transferId])
  @@index([pickupDate])
  @@index([status])
}

// Enums for Airport Transfer
enum VehicleType {
  SEDAN          // 1-3 passengers
  VAN            // 4-8 passengers
  MINIBUS        // 9-14 passengers
  BUS            // 15+ passengers
  LUXURY_SEDAN   // VIP Sedan
  LUXURY_VAN     // VIP Van
}

enum TransferType {
  ONE_WAY
  ROUND_TRIP
}

// Admin User Model
model Admin {
  id            String    @id @default(cuid())
  email         String    @unique
  passwordHash  String
  role          AdminRole @default(MODERATOR)
  permissions   String[]  // Array of permission strings
  isActive      Boolean   @default(true)
  lastLogin     DateTime?
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt

  @@map("admins")
  @@index([email])
  @@index([role])
}

enum AdminRole {
  SUPER_ADMIN
  ADMIN
  MODERATOR
}

// Price Tracking Models
model PriceHistory {
  id          String   @id @default(cuid())
  entityType  String   // HOTEL, FLIGHT, TOUR
  entityId    String   // External ID (hotel ID, flight ID, etc.)
  price       Decimal
  currency    String   @default("TRY")
  metadata    Json?    // Store additional data like room type, flight class, etc.
  source      String?  // Where the price was fetched from
  checkInDate DateTime? // For hotels
  checkOutDate DateTime? // For hotels
  travelDate  DateTime? // For flights/tours
  createdAt   DateTime @default(now())

  @@map("price_history")
  @@index([entityType, entityId])
  @@index([entityType, entityId, createdAt])
  @@index([createdAt])
}

model PriceAlert {
  id              String          @id @default(cuid())
  userId          String
  entityType      String          // HOTEL, FLIGHT, TOUR
  entityId        String          // External ID
  entityName      String          // Display name
  targetPrice     Decimal
  currentPrice    Decimal
  currency        String          @default("TRY")
  status          PriceAlertStatus @default(ACTIVE)
  notificationMethod String[]     @default(["EMAIL"]) // EMAIL, PUSH, SMS
  metadata        Json?           // Store search criteria, dates, etc.

  // Alert settings
  priceDropPercentage Decimal?    // Notify when price drops by X%
  expiresAt       DateTime?       // Auto-disable after this date
  lastNotifiedAt  DateTime?       // Track last notification sent
  notificationCount Int           @default(0)

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  user            User            @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@map("price_alerts")
  @@index([userId])
  @@index([entityType, entityId])
  @@index([status])
  @@index([expiresAt])
}

enum PriceAlertStatus {
  ACTIVE
  TRIGGERED
  EXPIRED
  DISABLED
}

// Trip Planner Models
model Trip {
  id              String          @id @default(cuid())
  userId          String          // Creator of the trip
  title           String
  description     String?         @db.Text
  destination     String
  startDate       DateTime
  endDate         DateTime
  budget          Decimal
  currency        String          @default("TRY")
  travelers       Int             @default(1)
  status          TripStatus      @default(DRAFT)
  preferences     Json?           // Travel preferences, interests
  aiGenerated     Boolean         @default(false)
  isPublic        Boolean         @default(false)
  shareLink       String?         @unique

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  // Relations
  user            User            @relation(fields: [userId], references: [id], onDelete: Cascade)
  itinerary       TripItinerary[]
  collaborators   TripCollaborator[]
  activities      TripActivity[]
  comments        TripComment[]
  votes           TripVote[]

  @@map("trips")
  @@index([userId])
  @@index([status])
  @@index([startDate])
  @@index([shareLink])
}

model TripItinerary {
  id              String          @id @default(cuid())
  tripId          String
  day             Int
  date            DateTime
  title           String
  description     String?         @db.Text
  activities      Json[]          // Array of activities with time, location, cost
  totalCost       Decimal         @default(0)
  weather         Json?           // Weather data
  notes           String?         @db.Text

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  trip            Trip            @relation(fields: [tripId], references: [id], onDelete: Cascade)

  @@map("trip_itineraries")
  @@index([tripId])
  @@index([day])
}

model TripCollaborator {
  id              String                  @id @default(cuid())
  tripId          String
  userId          String?                 // Null if invited by email not yet signed up
  email           String
  role            CollaboratorRole        @default(VIEWER)
  status          CollaboratorStatus      @default(PENDING)
  invitedBy       String
  invitedAt       DateTime                @default(now())
  joinedAt        DateTime?
  permissions     String[]                @default([])

  trip            Trip                    @relation(fields: [tripId], references: [id], onDelete: Cascade)

  @@unique([tripId, email])
  @@map("trip_collaborators")
  @@index([tripId])
  @@index([userId])
  @@index([email])
}

model TripActivity {
  id              String          @id @default(cuid())
  tripId          String
  title           String
  description     String?         @db.Text
  type            ActivityType
  location        String
  address         String?
  coordinates     Json?           // {lat, lng}
  startTime       DateTime
  endTime         DateTime?
  duration        Int?            // Duration in minutes
  cost            Decimal         @default(0)
  currency        String          @default("TRY")
  bookingUrl      String?
  bookingStatus   String?         @default("PENDING")
  notes           String?         @db.Text
  addedBy         String
  aiSuggested     Boolean         @default(false)

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  trip            Trip            @relation(fields: [tripId], references: [id], onDelete: Cascade)
  votes           TripVote[]

  @@map("trip_activities")
  @@index([tripId])
  @@index([type])
  @@index([startTime])
}

model TripComment {
  id              String          @id @default(cuid())
  tripId          String
  userId          String
  activityId      String?         // Optional: comment on specific activity
  content         String          @db.Text
  parentId        String?         // For nested comments
  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  trip            Trip            @relation(fields: [tripId], references: [id], onDelete: Cascade)

  @@map("trip_comments")
  @@index([tripId])
  @@index([userId])
  @@index([activityId])
  @@index([parentId])
}

model TripVote {
  id              String          @id @default(cuid())
  tripId          String
  activityId      String?         // Can vote on activities
  userId          String
  voteType        VoteType
  comment         String?

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  trip            Trip            @relation(fields: [tripId], references: [id], onDelete: Cascade)
  activity        TripActivity?   @relation(fields: [activityId], references: [id], onDelete: Cascade)

  @@unique([tripId, activityId, userId])
  @@map("trip_votes")
  @@index([tripId])
  @@index([activityId])
  @@index([userId])
}

// Trip Planner Enums
enum TripStatus {
  DRAFT
  PLANNING
  CONFIRMED
  IN_PROGRESS
  COMPLETED
  CANCELLED
}

enum CollaboratorRole {
  OWNER
  EDITOR
  VIEWER
}

enum CollaboratorStatus {
  PENDING
  ACTIVE
  DECLINED
  REMOVED
}

enum ActivityType {
  HOTEL
  FLIGHT
  RESTAURANT
  ATTRACTION
  TOUR
  TRANSPORTATION
  SHOPPING
  ENTERTAINMENT
  RELAXATION
  OTHER
}

enum VoteType {
  UPVOTE
  DOWNVOTE
  INTERESTED
  NOT_INTERESTED
}

// Hotel Models
model Hotel {
  id              String          @id @default(cuid())
  name            String
  slug            String          @unique
  description     String          @db.Text
  shortDescription String?        @db.Text

  // Location
  country         String          @default("Turkey")
  city            String
  region          String          // Antalya, Alanya, Belek, etc.
  address         String
  coordinates     Json?           // {lat, lng}
  distanceToAirport Float?        // km
  distanceToBeach Float?          // km
  distanceToCenter Float?         // km

  // Hotel Info
  stars           Int             // 1-5
  rating          Decimal         @default(0)
  reviewCount     Int             @default(0)
  hotelType       HotelType
  checkInTime     String          @default("14:00")
  checkOutTime    String          @default("12:00")
  yearBuilt       Int?
  yearRenovated   Int?
  roomCount       Int
  floorCount      Int?

  // Images
  mainImage       String
  images          String[]        @default([])
  virtualTourUrl  String?

  // Features & Amenities
  amenities       String[]        @default([])
  facilities      String[]        @default([])
  roomFeatures    String[]        @default([])

  // Policies
  cancellationPolicy String?      @db.Text
  childPolicy     String?         @db.Text
  petPolicy       String?         @db.Text

  // Pricing
  priceMin        Decimal         // Minimum room price per night
  priceMax        Decimal         // Maximum room price per night
  currency        String          @default("TRY")

  // Contact
  phone           String?
  email           String?
  website         String?

  // Status
  isActive        Boolean         @default(true)
  isFeatured      Boolean         @default(false)
  isRecommended   Boolean         @default(false)

  // SEO
  metaTitle       String?
  metaDescription String?
  keywords        String[]        @default([])

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  // Relations
  rooms           HotelRoom[]

  @@map("hotels")
  @@index([city])
  @@index([region])
  @@index([stars])
  @@index([rating])
  @@index([isActive])
  @@index([isFeatured])
}

model HotelRoom {
  id              String          @id @default(cuid())
  hotelId         String

  // Room Info
  name            String
  slug            String
  description     String          @db.Text
  roomType        RoomType
  size            Float?          // Square meters
  bedType         String          // King, Queen, Twin, etc.
  maxOccupancy    Int
  maxAdults       Int
  maxChildren     Int

  // Images
  images          String[]        @default([])

  // Amenities
  amenities       String[]        @default([])
  view            String[]        @default([]) // Sea view, Mountain view, etc.

  // Pricing
  pricePerNight   Decimal
  currency        String          @default("TRY")

  // Availability
  roomCount       Int             // Number of this room type
  isAvailable     Boolean         @default(true)

  // Meal Options
  mealPlans       String[]        @default([]) // Room Only, Breakfast, Half Board, Full Board, All Inclusive

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  // Relations
  hotel           Hotel           @relation(fields: [hotelId], references: [id], onDelete: Cascade)

  @@map("hotel_rooms")
  @@index([hotelId])
  @@index([roomType])
  @@index([isAvailable])
}

// Hotel Enums
enum HotelType {
  RESORT
  BOUTIQUE
  CITY_HOTEL
  BEACH_HOTEL
  APARTMENT
  VILLA
  HOSTEL
  GUESTHOUSE
}

enum RoomType {
  STANDARD
  DELUXE
  SUITE
  FAMILY
  PENTHOUSE
  VILLA
  BUNGALOW
}

// Flight Models
model Flight {
  id              String          @id @default(cuid())

  // Flight Info
  flightNumber    String
  airline         String
  airlineCode     String          // TK, PC, XQ, etc.
  airlineLogo     String?

  // Departure
  departureAirport String         // AYT, IST, SAW, etc.
  departureCity   String
  departureCountry String         @default("Turkey")
  departureTime   DateTime
  departureTerminal String?

  // Arrival
  arrivalAirport  String
  arrivalCity     String
  arrivalCountry  String
  arrivalTime     DateTime
  arrivalTerminal String?

  // Flight Details
  duration        Int             // Minutes
  distance        Float?          // KM
  aircraft        String?         // Boeing 737, Airbus A320, etc.
  flightType      FlightType
  cabinClass      CabinClass

  // Stops
  stops           Int             @default(0)
  stopAirports    String[]        @default([])

  // Pricing
  priceAdult      Decimal
  priceChild      Decimal
  priceInfant     Decimal
  currency        String          @default("TRY")
  taxIncluded     Boolean         @default(true)

  // Baggage
  carryOnBaggage  String?         // "1x8kg"
  checkedBaggage  String?         // "1x23kg" or "2x23kg"
  extraBaggagePrice Decimal?

  // Seat & Services
  availableSeats  Int
  mealService     Boolean         @default(false)
  wifiAvailable   Boolean         @default(false)
  entertainmentSystem Boolean     @default(false)
  powerOutlets    Boolean         @default(false)

  // Booking
  isRefundable    Boolean         @default(false)
  cancellationFee Decimal?
  changeFee       Decimal?

  // Status
  isActive        Boolean         @default(true)
  operatingDays   String[]        @default([]) // ["Mon", "Wed", "Fri"]

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  @@map("flights")
  @@index([departureAirport, arrivalAirport])
  @@index([departureTime])
  @@index([airline])
  @@index([flightType])
  @@index([isActive])
}

// Flight Enums
enum FlightType {
  DOMESTIC
  INTERNATIONAL
}

enum CabinClass {
  ECONOMY
  PREMIUM_ECONOMY
  BUSINESS
  FIRST_CLASS
}

// Tour Package Models
model TourPackage {
  id              String          @id @default(cuid())

  // Basic Info
  name            String
  slug            String          @unique
  description     String          @db.Text
  shortDescription String?        @db.Text

  // Category
  category        TourCategory
  tags            String[]        @default([])

  // Location
  destination     String
  destinations    String[]        @default([]) // Multiple destinations
  region          String
  country         String          @default("Turkey")

  // Duration
  duration        Int             // Days
  nights          Int

  // Group
  minGroupSize    Int             @default(1)
  maxGroupSize    Int

  // Pricing
  priceAdult      Decimal
  priceChild      Decimal
  priceInfant     Decimal
  currency        String          @default("TRY")

  // Inclusions
  includes        String[]        @default([])
  excludes        String[]        @default([])

  // Itinerary (stored as JSON array)
  itinerary       Json            // Daily program

  // Schedule
  availableDays   String[]        @default([]) // Days of week
  startDates      DateTime[]      @default([])

  // Images
  mainImage       String
  images          String[]        @default([])
  videoUrl        String?

  // Difficulty & Physical
  difficultyLevel DifficultyLevel @default(EASY)
  physicalRating  Int             @default(1) // 1-5

  // Requirements
  minimumAge      Int?
  requiredDocs    String[]        @default([])

  // Meals
  mealsIncluded   String[]        @default([]) // Breakfast, Lunch, Dinner

  // Transportation
  transportType   String?         // Bus, Car, Boat, etc.
  pickupIncluded  Boolean         @default(false)
  pickupLocations String[]        @default([])

  // Guide
  guideLanguages  String[]        @default([])
  guideIncluded   Boolean         @default(true)

  // Status & Featured
  isActive        Boolean         @default(true)
  isFeatured      Boolean         @default(false)
  isPopular       Boolean         @default(false)

  // Ratings
  rating          Decimal         @default(0)
  reviewCount     Int             @default(0)

  // SEO
  metaTitle       String?
  metaDescription String?
  keywords        String[]        @default([])

  // Cancellation
  cancellationPolicy String?      @db.Text
  refundable      Boolean         @default(true)

  createdAt       DateTime        @default(now())
  updatedAt       DateTime        @updatedAt

  @@map("tour_packages")
  @@index([category])
  @@index([destination])
  @@index([region])
  @@index([isActive])
  @@index([isFeatured])
  @@index([rating])
}

